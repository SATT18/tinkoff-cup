{
  "name": "tinkoff-cup",
  "private": true,
  "version": "0.0.1",
  "homepage": "https://satt18.github.io/tinkoff-cup",
  "type": "module",
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "start": "node load-schema.js && npm run generate-swagger-client && vite",
    "dev": "vite",
    "load-schemas": "node load-schema.js",
    "build": "vite build",
    "preview": "vite preview",
    "generate-swagger-client": "orval --config orval.config.cjs",
    "ts:build": "tsc --noEmit",
    "format:prettier": "prettier --write \"src/**/*.{ts,tsx}\" package.json",
    "format:eslint": "eslint --color --fix --cache \"src/**/*.{ts,tsx}\"",
    "format": "npm run format:prettier && npm run format:eslint",
    "lint": "eslint ./src"
  },
  "dependencies": {
    "@ant-design/icons": "^5.0.1",
    "antd": "^5.4.3",
    "axios": "^1.3.6",
    "classnames": "^2.3.2",
    "dayjs": "^1.11.7",
    "fs": "^0.0.1-security",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-query": "^3.39.3",
    "react-router": "^6.10.0",
    "react-router-dom": "^6.10.0",
    "recoil": "^0.7.7"
  },
  "devDependencies": {
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^3.1.0",
    "eslint": "^8.38.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-scope": "^7.2.0",
    "orval": "^6.14.3",
    "prettier": "^2.8.7",
    "stylelint": "^14.0.0",
    "stylelint-config-css-modules": "^4.2.0",
    "stylelint-config-recommended-scss": "^10.0.0",
    "stylelint-config-standard": "^33.0.0",
    "stylelint-order": "^6.0.3",
    "stylelint-value-no-unknown-custom-properties": "^4.0.0",
    "typescript": "^4.9.3",
    "typescript-plugin-css-modules": "^5.0.1",
    "vite": "^4.2.0",
    "vite-plugin-sass-dts": "^1.3.4",
    "vite-plugin-svgr": "^2.4.0"
  }
}
